<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/advancedoption_parent_layout"
    tools:context=".Dashboard.AdvancedOptionActivity">
    <include
        android:id="@+id/advancedoption_toolbar"
        layout="@layout/toolbar"/>


    <ScrollView
        android:layout_below="@+id/advancedoption_toolbar"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
        <RelativeLayout
            android:id="@+id/indivijuallensdata_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:orientation="vertical">
        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/indivijuallensdata"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/indivijuallensdata"
            android:textSize="@dimen/text_medium"
            android:textStyle="bold"
            android:textAllCaps="true"
            android:textColor="@color/colorPrimaryDark"/>

            <LinearLayout
                android:id="@+id/indivijuallensdata_childheader"
                android:layout_below="@+id/indivijuallensdata"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/fab_margin"
                android:orientation="horizontal">

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/pantoangle"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/framefit"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/bowangle"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/distancenear"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/lensdata_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/text_msmall"
                android:layout_below="@+id/indivijuallensdata_childheader">


                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/pantoangle_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/framefit_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:layout_marginLeft="@dimen/text_two_dp"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/bowangle_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>


                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/distancenear_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/lenstype_child_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_msmall"
                android:orientation="horizontal"
                android:layout_below="@+id/lensdata_input_layout">
                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:text="@string/neartype"
                    android:maxLines="1"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:text="@string/mid"
                    android:layout_marginLeft="@dimen/text_two_dp"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:text="@string/indivijualengrav"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:maxLines="1"
                    android:paddingLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/lenstype_child_inputs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/text_vsmall"
                android:orientation="horizontal"
                android:layout_below="@+id/lenstype_child_header">
                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/neartype_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/mid_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:layout_marginLeft="@dimen/text_two_dp"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/indivijuallengrav_lenstype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:layout_marginLeft="@dimen/text_two_dp"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/indijual_valuessteps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_blue_dark"
                android:layout_below="@+id/lenstype_child_inputs"
                android:layout_marginTop="@dimen/text_vsmall"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/indijual_error_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_red_dark"
                android:layout_below="@+id/indijual_valuessteps"
                android:layout_marginTop="@dimen/text_vsmall"/>

        </RelativeLayout>
            <View
                android:id="@+id/lens_view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/text_four_dp"
                android:layout_below="@+id/indivijuallensdata_layout"
                android:background="@color/colorPrimary"/>




        <RelativeLayout
            android:id="@+id/framedata_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:orientation="vertical"
            android:layout_below="@+id/lens_view">
            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/framedata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/framedata"
                android:textSize="@dimen/text_medium"
                android:textStyle="bold"
                android:textAllCaps="true"
                android:textColor="@color/colorPrimaryDark"/>

            <android.support.v7.widget.AppCompatSpinner
                android:id="@+id/framedata_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framedata"
                android:layout_marginTop="@dimen/fab_margin"
                />


            <LinearLayout
                android:id="@+id/framelength_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framedata_list"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text=""
                    android:paddingLeft="@dimen/fab_margin"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/framelength"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/frameheight"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/dbl"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/framelength_values_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framelength_header"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text="@string/r"
                    android:padding="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/framelength_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/frameheight_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/dbl_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/framelength_values_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/framelength_values_right"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text="@string/l"
                    android:padding="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/framelength_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/frameheight_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/dbl_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>



            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/framedata_valuessteps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_blue_dark"
                android:layout_below="@+id/framelength_values_left"
                android:layout_marginTop="@dimen/text_vsmall"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/framedata_error_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_red_dark"
                android:layout_below="@+id/framedata_valuessteps"
                android:layout_marginTop="@dimen/text_vsmall"/>
        </RelativeLayout>


            <View
                android:id="@+id/framedata_view"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/text_large"
                android:layout_below="@+id/framedata_layout"
                android:background="@color/colorPrimary"/>


        <RelativeLayout
            android:id="@+id/centrationdata_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/fab_margin"
            android:orientation="vertical"
            android:layout_below="@+id/framedata_view">

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/centrationdata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/centrationdata"
                android:textSize="@dimen/text_medium"
                android:textStyle="bold"
                android:textAllCaps="true"
                android:textColor="@color/colorPrimaryDark"/>



            <LinearLayout
                android:id="@+id/centrationdata_header"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/centrationdata"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text=""
                    android:paddingLeft="@dimen/fab_margin"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/pdz"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/yfh"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/bvd"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/centrationdata_values_right"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/centrationdata_header"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text="@string/r"
                    android:padding="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/pdz_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:inputType="numberDecimal|numberSigned"
                    android:maxLines="1"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/yfh_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/bvd_right"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/centrationdata_values_left"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/centrationdata_values_right"
                android:layout_marginTop="@dimen/fab_margin">


                <android.support.v7.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:text="@string/l"
                    android:padding="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/pdz_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/yfh_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>

                <android.support.v7.widget.AppCompatEditText
                    android:id="@+id/bvd_left"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/text_msmall"
                    android:layout_weight="1"
                    android:background="@drawable/rectangle"
                    android:maxLines="1"
                    android:inputType="numberDecimal|numberSigned"
                    android:layout_marginLeft="@dimen/text_four_dp"
                    android:textColor="@color/colorPrimaryDark"/>
            </LinearLayout>


            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/centrationdata_valuessteps"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_blue_dark"
                android:layout_below="@+id/centrationdata_values_left"
                android:layout_marginTop="@dimen/text_vsmall"/>

            <android.support.v7.widget.AppCompatTextView
                android:id="@+id/centrationdata_error_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@android:color/holo_red_dark"
                android:layout_below="@+id/centrationdata_valuessteps"
                android:layout_marginTop="@dimen/text_vsmall"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/centrationdata_error_txt"
                android:layout_marginTop="@dimen/fab_margin"
                android:layout_marginBottom="@dimen/fab_margin">
                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/advancedoption_cancel"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="@string/cancel"
                    android:background="@drawable/custombtn_bg"
                    android:layout_marginRight="2dp"
                    android:textColor="@color/colorPrimaryDark"
                    />
                <android.support.v7.widget.AppCompatButton
                    android:id="@+id/advancedoption_confirm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:background="@drawable/custombtn_bg"
                    android:text="@string/confirm"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_marginLeft="2dp"/>

            </LinearLayout>
        </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>